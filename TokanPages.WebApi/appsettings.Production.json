{
  "AllowedHosts": "*",
  "SmtpServer": 
  {
    "Account": "{{Account}}",
    "Password": "{{Password}}",
    "Port": "{{Port}}",
    "Server": "{{Server}}",
    "IsSSL": true
  },
  "ConnectionStrings": 
  {
    "DbConnect": "{{DbConnect}}",
    "DbConnectTest": "{{DbConnect}}"
  },
  "AzureStorage": 
  {
    "BaseUrl": "{{BaseUrl}}",
    "ContainerName": "{{ContainerName}}",
    "ConnectionString": "{{ConnectionString}}"
  },
  "AppUrls": 
  {
    "UpdateSubscriberPath": "{{UpdateSubscriberPath}}",
    "UnsubscribePath": "{{UnsubscribePath}}",
    "ResetPath": "{{ResetPath}}",
    "DevelopmentOrigin": "{{DevelopmentOrigin}}",
    "DeploymentOrigin": "{{DeploymentOrigin}}"
  },
  "Sentry": 
  {
    "Dsn": "{{Dsn}}"
  },
  "SonarQube":
  {
    "Server": "{{Server}}",
    "Token": "{{Token}}"
  },
  "IdentityServer":
  {
    "Issuer": "{{Authority}}",
    "Audience": "{{Audience}}",
    "WebSecret": "{{WebSecret}}",
    "RequireHttps": true,
    "WebTokenExpiresIn": "{{WebTokenExpiresIn}}",
    "RefreshTokenExpiresIn": "{{RefreshTokenExpiresIn}}"
  }
}
