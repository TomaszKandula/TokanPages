{
  "AllowedHosts": "*",
  "EmailSender":
  {
    "PrivateKey": "{{PrivateKey}}",
    "BaseUrl": "{{BaseUrl}}"
  },
  "ConnectionStrings": 
  {
    "DbConnect": "{{DbConnect}}",
    "DbConnectTest": "{{DbConnect}}"
  },
  "AzureStorage": 
  {
    "BaseUrl": "{{BaseUrl}}",
    "ContainerName": "{{ContainerName}}",
    "ConnectionString": "{{ConnectionString}}",
    "MaxFileSizeUserMedia": 104857600,
    "MaxFileSizeSingleAsset": 262144000
  },
  "AzureRedis":
  {
    "InstanceName": "{{InstanceName}}",
    "ConnectionString": "{{ConnectionString}}",
    "ExpirationMinute": 15,
    "ExpirationSecond": 30
  },
  "ApplicationPaths": 
  {
    "UpdateSubscriberPath": "{{UpdateSubscriberPath}}",
    "UnsubscribePath": "{{UnsubscribePath}}",
    "UpdatePasswordPath": "{{UpdatePasswordPath}}",
    "ActivationPath": "{{ActivationPath}}",
    "DevelopmentOrigin": "{{DevelopmentOrigin}}",
    "DeploymentOrigin": "{{DeploymentOrigin}}"
  },
  "SonarQube":
  {
    "Server": "{{Server}}",
    "Token": "{{Token}}"
  },
  "IdentityServer":
  {
    "Issuer": "{{Authority}}",
    "Audience": "{{Audience}}",
    "WebSecret": "{{WebSecret}}",
    "RequireHttps": true,
    "WebTokenExpiresIn": "{{WebTokenExpiresIn}}",
    "RefreshTokenExpiresIn": "{{RefreshTokenExpiresIn}}"
  },
  "ExpirationSettings":
  {
    "ResetIdExpiresIn": "{{ResetIdExpiresIn}}",
    "ActivationIdExpiresIn": "{{ActivationIdExpiresIn}}"
  }
}
