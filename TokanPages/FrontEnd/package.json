{
  "name": "tomkandula",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "aos": "^2.3.4",
    "axios": "^0.21.0",
    "browserslist": "^4.14.5",
    "caniuse-lite": "^1.0.30001148",
    "connected-react-router": "6.5.2",
    "history": "4.10.1",
    "husky": "^4.3.0",
    "merge": "1.2.1",
    "react-html-parser": "^2.0.2",
    "react-redux": "^7.1.1",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "^3.4.1",
    "reactstrap": "8.7.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "svgo": "1.3.0"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@types/aos": "^3.0.3",
    "@types/domhandler": "^2.4.1",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "4.7.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-html-parser": "^2.0.1",
    "@types/react-redux": "7.1.1",
    "@types/react-router-dom": "^5.1.6",
    "@types/reactstrap": "8.7.1",
    "@typescript-eslint/parser": "^2.5.0",
    "cross-env": "6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^6.5.1",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "nan": "^2.14.1",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  },
  "resolutions": {
    "**/react": "17.0.1",
    "**/react-dom": "17.0.1"
  },
  "scripts": {
    "shared-test": "jest --verbose ./Shared",
    "comp-test": "jest --verbose ./Components",
    "app-test": "cross-env CI=true react-scripts test --env=jsdom --verbose",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.ts ./src/**/*.tsx"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "*/**/*.ts",
      "*/**/*.tsx"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!lodash-es/).+\\.js$"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.ts",
      "^.+\\.(css|less|scss|sass)$": "./__mocks__/styleMock.ts"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-commit": "git update-index -g"
    }
  }
}
