{
  "AllowedHosts": "*",
  "SmtpServer": 
  {
    "Account": "{{Account}}",
    "Password": "{{Password}}",
    "Port": "{{Port}}",
    "Server": "{{Server}}",
    "IsSSL": true
  },
  "ConnectionStrings": 
  {
    "DbConnect": "{{DbConnect}}"
  },
  "AzureStorage": 
  {
    "AccountName": "{{AccountName}}",
    "AccountKey": "{{AccountKey}}",
    "BaseUrl": "https://{AccountName}.blob.core.windows.net/{ContainerName}",
    "ContainerName": "{{ContainerName}}"
  },
  "AppUrls": 
  {
    "UpdateSubscriberPath": "{{UpdateSubscriberPath}}",
    "UnsubscribePath": "{{UnsubscribePath}}",
    "DevelopmentOrigin": "{{DevelopmentOrigin}}",
    "DeploymentOrigin": "{{DeploymentOrigin}}"
  },
  "Sentry": 
  {
    "Dsn": "{{Dsn}}"
  }
}
